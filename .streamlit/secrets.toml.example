# Example secrets.toml file for Agentic RAG
# Rename this file to 'secrets.toml' and add your actual API keys

# Required: Groq API key for LLM functionality
# Get one from: https://console.groq.com/
GROQ_API_KEY = "your-groq-api-key-here"

# Optional: Google API key for Gemini embeddings 
# If not provided, the app will use hash-based embeddings
GOOGLE_API_KEY = "your-google-api-key-here"
